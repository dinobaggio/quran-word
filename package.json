{
  "name": "simple-starterkit",
  "version": "2.0.0",
  "author": "Dino Baggio",
  "private": true,
  "scripts": {
    "start": "node ./dist/bin/server.js",
    "dev": "nodemon",
    "build": "rm -R ./dist;babel src --out-dir dist",
    "serve": "node ./dist/bin/server.js"
  },
  "nodemonConfig": {
    "exec": "babel-node ./src/bin/server.js",
    "ext": "js,json,hbs,html",
    "watch": [
      "src/*",
      "public/*",
      "views/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      [
        "@babel/transform-runtime"
      ]
    ]
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.0.0",
    "express": "~4.16.1",
    "express-validator": "^6.14.2",
    "hbs": "^4.2.0",
    "http-errors": "~1.6.3",
    "i18n": "^0.15.1",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "morgan": "~1.9.1",
    "mysql2": "^2.3.3",
    "sequelize": "^6.26.0",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/register": "^7.11.5",
    "babel-node": "0.0.1-security",
    "eslint": "^7.10.0"
  }
}
