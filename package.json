{
  "name": "simple-starterkit",
  "version": "2.0.0",
  "author": "Dino Baggio",
  "private": true,
  "scripts": {
    "start": "node ./dist/bin/server.js",
    "dev": "nodemon",
    "build": "sh build",
    "serve": "node ./dist/bin/server.js",
    "test": "mocha tests --require @babel/register --sort --exit",
    "test:file": "mocha --require @babel/register --sort --exit",
    "dev:listener": "babel-node ./src/bin/amqpListener.js",
    "start:listener": "node ./dist/bin/amqpListener.js"
  },
  "nodemonConfig": {
    "exec": "babel-node ./src/bin/server.js",
    "ext": "js,json,hbs,html",
    "watch": [
      "src/*",
      "public/*",
      "views/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      [
        "@babel/transform-runtime"
      ]
    ]
  },
  "dependencies": {
    "axios": "^1.2.2",
    "bcrypt": "^5.1.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.0.0",
    "express": "~4.16.1",
    "express-validator": "^6.14.2",
    "hbs": "^4.2.0",
    "http-errors": "~1.6.3",
    "i18n": "^0.15.1",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "morgan": "~1.9.1",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/register": "^7.11.5",
    "@faker-js/faker": "^7.6.0",
    "babel-node": "0.0.1-security",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^7.10.0",
    "mocha": "^8.3.0",
    "mocha-junit-reporter": "^2.0.0",
    "mochawesome": "^6.1.1",
    "nyc": "^15.1.0"
  }
}
