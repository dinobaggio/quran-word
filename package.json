{
  "name": "proxy-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./dist/bin/server.js",
    "dev": "nodemon",
    "build": "rm -R ./dist;babel src --out-dir dist",
    "serve": "node ./dist/bin/server.js"
  },
  "nodemonConfig": {
    "exec": "babel-node ./src/bin/server.js",
    "ext": "js,json,hbs,html",
    "watch": [
      "src/*",
      "public/*",
      "views/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      [
        "@babel/transform-runtime"
      ]
    ]
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.0.0",
    "express": "~4.16.1",
    "hbs": "^4.2.0",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/register": "^7.11.5",
    "babel-node": "0.0.1-security",
    "eslint": "^7.10.0"
  }
}
